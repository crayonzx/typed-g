{
  "compilerOptions": {
      "outDir": "./lib",
      "module": "commonjs",
      // "allowJs": true,
      "sourceMap": true,
      "declaration": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      // "noImplicitAny": true,
      "strictNullChecks": true,
      "target": "es5"
  },
  "include": ["./ts-src/**/*"]
}
